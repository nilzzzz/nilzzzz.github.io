<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>#include&lt;iostream&gt;
using namespace std;
int main(){
	int count=0;
	for(int a=1;a&lt;10;a++){
		for(int b=1;b&lt;10;b++){
			if(a==b)continue;
			for(int c=1;c&lt;10;c++){
				if(a==c||b==c)continue;
				for(int d=1;d&lt;10;d++){
					if(a==d||b==d||c==d)continue;
					for(int e=1;e&lt;10;e++){
						if(a==e||b==e||c==e||d==e)continue;
						for(int f=1;f&lt;10;f++){
							if(a==f||b==f||c==f||d==f||e==f)continue;
							for(int g=1;g&lt;10;g++){
								if(a==g||b==g||c==g||d==g||e==g||f==g)continue;
								for(int h=1;h&lt;10;h++){
									if(a==h||b==h||c==h||d==h||e==h||f==h||g==h)continue;
									for(int i=1;i&lt;10;i++){
										if(a==i||b==i||c==i||d==i||e==i||f==i||g==i||h==i)continue;
										int t1=a*c*(g*100+h*10+i);
										int t2=b*(g*100+h*10+i);
										int t3=c*(d*100+e*10+f);
										int t=10*c*(g*100+h*10+i);
										if(t1+t2+t3==t)count++;
									}
								}
							}
						}
					}
				}
			}
		}
	}
	cout&lt;&lt;count&lt;&lt;endl;
	return 0;
}
</code></pre></div></div>
<h3 id="2adv-205ç®—æ³•æé«˜-æ‹¿ç³–æœåŠ¨æ€è§„åˆ’">2.ADV-205ç®—æ³•æé«˜ æ‹¿ç³–æœï¼ˆåŠ¨æ€è§„åˆ’ï¼‰</h3>

<p>é—®é¢˜æè¿°
ã€€ã€€å¦ˆå¦ˆç»™å°Bä¹°äº†Nå—ç³–ï¼ä½†æ˜¯å¥¹ä¸å…è®¸å°Bç›´æ¥åƒæ‰ã€‚<br />
